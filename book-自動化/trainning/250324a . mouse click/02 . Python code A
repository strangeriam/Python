
;# Python code A
;# ==============
buttonNormal=(pyautogui.click(500,240, button='left', duration=0.25, clicks=2, interval=1))
buttonActive=(pyautogui.click(500,270, button='left', duration=0.25, clicks=2, interval=1))

buttonNormal ; print (pyautogui.position())
# time.sleep(1)
buttonActive ; print (pyautogui.position())

;# Python code B
;# ==============
buttonNormal=(pyautogui.click(500,240, button='left', duration=0.25))
buttonActive=(pyautogui.click(500,270, button='left', duration=0.25))

buttonNormal ; print (pyautogui.position())
# time.sleep(1)
buttonActive ; print (pyautogui.position())
