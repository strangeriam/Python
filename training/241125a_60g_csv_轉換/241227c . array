from pathlib import Path
import csv

fname = 'PSO_241126.csv'
csvFile = open(Path(r'D:\Dropbox\12-Office-TryTryLu\Learning_02_Python\241211b_csv_60g_sector2', fname))
csvReader = csv.reader(csvFile)
csvData = list(csvReader)

;# 取出第 1 行內容
csvData[1]

;# 輸出
1,1,-32,-20,90,270,90,0,270,0,270,0,0,0,0,180,90,270,180,0,180,90,270,90,0,90,270,90,90,0,180,90,0,0,0,0

;# 取出 所有 phase. 從第 4 個值到 最後 1 個值.
listPhase = (csvData[1])[4:]
print(listPhase)
;# 輸出
['90', '270', '90', '0', '270', '0', '270', '0', '0', '0', '0', '180', '90', '270', '180', '0', '180', '90', '270', '90', '0', '90', '270', '90', '90', '0', '180', '90', '0', '0', '0', '0']
