import pyautogui
import time

wh = pyautogui.size() # Obtain the screen resolution
print (wh)

for i in range(10):
    pyautogui.moveTo(-2078,170 , duration=0.25)
    pyautogui.moveTo(-2078,200 , duration=0.25)
    pyautogui.moveTo(-2078,170 , duration=0.25)
    pyautogui.moveTo(-2078,200 , duration=0.25)

pyautogui.click(-2000,30, button='left', duration=0.25)
time.sleep(1)
pyautogui.click(-2000,60, button='left', duration=0.25)
